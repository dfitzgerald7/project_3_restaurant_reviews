
<h1> Welcome to RESTaurants! </h1>
<h2> Timeline of recent reviews: </h2>
<ul>
<% @reviews.each do |review| %>
  <li> Restaurant: <%= link_to review.restaurant.name, restaurant_path(review.restaurant)%> </li>
  <li> Rating: <%= review.rating %> </li>
  <li> Review: <%= review.content %> </li>
  <br>
<% end %>
</ul>

<% if logged_in? %>
  <% link_to "Have you eaten anywhere recently"%> 

<% end %>
