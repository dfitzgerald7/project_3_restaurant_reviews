<%= form_for @review do |f| %>
  <%= f.hidden_field :restaurant_id %>
  <%= f.label :restaurant %>
  
  <%= f.text_field :restaurant, value: <%=@restaurant ? @restaurant.name : nil%> %>
  <%= f.label :rating  %>
  <%= f.text_field :rating %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
